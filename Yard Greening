import java.util.Scanner;

public class YardGreening {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        double yardGreening = Double.parseDouble(scanner.nextLine());
        
        double sumYard = yardGreening * 7.61;
        double discount = sumYard * 0.18;
        double totalPrice = sumYard - discount;

        System.out.printf("The final price is: %.2f lv.%n", totalPrice);
        System.out.printf("The discount is: %.2f lv.", discount);
    }
}
